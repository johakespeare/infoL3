def separer(L):
    taille = len(L)
    LSPE =[]
    for i in range (0,taille):
        LSPE.append(0)
    for i in range(0,taille):
        if L[i]>0 :
            LSPE.append(L[])
        if L[i]<0 :
            LSPE.append(L[i])
